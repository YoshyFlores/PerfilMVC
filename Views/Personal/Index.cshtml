
@{
    ViewData["Title"] = "Personal";
}


@model Personal
@Html.LabelFor(m=>m.name, "NAME: ") @Model.name


@Html.LabelFor(m=>m.lastname, "LASTNAME: ") @Model.lastname


@Html.LabelFor(m=>m.age, "AGE: ") @Model.age


@Html.LabelFor(m=>m.telefono, "TELEFONO: ") @Model.telefono


@Html.LabelFor(m=>m.direccion, "DIRECCION: ") @Model.direccion


@Html.LabelFor(m=>m.correo, "CORREO: ") @Model.correo


